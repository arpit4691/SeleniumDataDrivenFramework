pipeline {
    agent any
    stages

{
    stage('Clone sources') {
    steps{
        git url: 'https://github.com/arpit4691/SeleniumDataDrivenFramework.git'
    }
}
    stage('Artifactory configuration') {
    steps{
        // Tool name from Jenkins configuration
        Artifactory.newMavenBuild().tool = "Maven-3.3.9"
        // Set Artifactory repositories for dependencies resolution and artifacts deployment.
        Artifactory.newMavenBuild().deployer releaseRepo:'libs-release-local', snapshotRepo:'libs-snapshot-local', server: server
        Artifactory.newMavenBuild().resolver releaseRepo:'libs-release', snapshotRepo:'libs-snapshot', server: server
    }
    }

    stage('Maven build') {
    steps{
        Artifactory.newMavenBuild().run pom: 'DataDrivenFramework/pom.xml', goals: 'clean install'
    }
    }

    stage('Publish build info') {
    steps{
        server.publishBuildInfo buildInfo
    }
    }
}
}
}